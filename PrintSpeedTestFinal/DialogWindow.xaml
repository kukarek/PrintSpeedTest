<Window x:Class="PrintSpeedTestFinal.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PrintSpeedTestFinal"
        mc:Ignorable="d"
        Title="DialogWindow" Height="200" Width="300" ResizeMode="NoResize" FontSize="18" WindowStartupLocation="CenterScreen" Background="#455F80" WindowStyle="None">
    <Grid x:Name="Grid1">
        <Grid.RowDefinitions>
            <RowDefinition  Height="0.4*"/>
            <RowDefinition Height="0.6*"/>
            <RowDefinition  Height="0.6*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Label Content="Приготовься печатать текст." Grid.Row="1" HorizontalAlignment="Center" Foreground="White"/>
        <Label Content="Три, два, один, старт!" Grid.Row="2" HorizontalAlignment="Center" Foreground="White"/>
        <Button Content="Начать печатать" Grid.Row="3" Margin="40,10,40,10" Background="White"  Click="Button_Click" Foreground="#455F80"/>
    </Grid>
</Window>
